void setup(){
  size(1500,1100);
   background(255,201,221);
   strokeWeight(8);
}
  
void draw() {
  
 
  if (mousePressed) {
 line(mouseX,mouseY,pmouseX,pmouseY);
  }
 if(keyPressed) {
  if (key == 'P' || key == 'p') {
  stroke(0,255,0);
}
 }
  if(keyPressed) {
  if (key == 'q' || key == 'Q'){
  stroke(0,0,0);
}
  }
if (keyPressed){
  if (key == 'o' || key == 'O'){
    fill(255,201,221);
    ellipse(mouseX,mouseY, 20, 20);
    


}
}
}
